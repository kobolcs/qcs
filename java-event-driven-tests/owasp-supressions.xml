<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

  <!-- Suppress Spring Framework CVEs that are not applicable to our use case -->
  <suppress>
    <notes><![CDATA[
    CVE-2024-38820: Spring Framework WebMVC vulnerability - not applicable as we're not using WebMVC in this test project
    ]]></notes>
    <packageUrl regex="true">^pkg:maven/org\.springframework/spring\-.*@.*$</packageUrl>
    <cve>CVE-2024-38820</cve>
  </suppress>

  <suppress>
    <notes><![CDATA[
    CVE-2025-22233: Spring Framework vulnerability - test project with controlled environment
    ]]></notes>
    <packageUrl regex="true">^pkg:maven/org\.springframework/spring\-.*@.*$</packageUrl>
    <cve>CVE-2025-22233</cve>
  </suppress>

  <!-- Suppress Kafka client CVEs for test environment -->
  <suppress>
    <notes><![CDATA[
    Kafka CVEs are for test containers only, not production deployment
    ]]></notes>
    <packageUrl regex="true">^pkg:maven/org\.apache\.kafka/kafka\-.*@.*$</packageUrl>
    <cve>CVE-2025-27817</cve>
  </suppress>

  <suppress>
    <notes><![CDATA[
    Kafka CVEs are for test containers only, not production deployment
    ]]></notes>
    <packageUrl regex="true">^pkg:maven/org\.apache\.kafka/kafka\-.*@.*$</packageUrl>
    <cve>CVE-2025-27818</cve>
  </suppress>

  <suppress>
    <notes><![CDATA[
    Kafka CVEs are for test containers only, not production deployment
    ]]></notes>
    <packageUrl regex="true">^pkg:maven/org\.apache\.kafka/kafka\-.*@.*$</packageUrl>
    <cve>CVE-2024-56128</cve>
  </suppress>

</suppressions>